{% extends "FOSUserBundle::layout.html.twig" %}

{% block body -%}
    <h1>Followcourse</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Idfollowcourse</th>
                <td>{{ entity.idFollowcourse }}</td>
            </tr>
            <tr>
                <th>Iduser</th>
                <td>{{ entity.idUser }}</td>
            </tr>
            <tr>
                <th>Note</th>
                <td>{{ entity.note }}</td>
            </tr>
            <tr>
                <th>Inscriptiondate</th>
                <td>{{ entity.inscriptiondate|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('followcourse') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('followcourse_edit', { 'id': entity.idFollowcourse }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
