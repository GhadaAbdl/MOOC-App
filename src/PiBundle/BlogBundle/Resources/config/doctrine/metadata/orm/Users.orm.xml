<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Users" table="users">
    <id name="idUser" type="integer" column="id_user">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="50" nullable="false"/>
    <field name="surname" type="string" column="surname" length="50" nullable="false"/>
    <field name="birthDate" type="date" column="birth_date" nullable="false"/>
    <field name="email" type="string" column="email" length="50" nullable="false"/>
    <field name="password" type="string" column="password" length="50" nullable="false"/>
    <field name="enabled" type="integer" column="enabled" nullable="false"/>
    <field name="level" type="string" column="level" length="50" nullable="false"/>
    <field name="type" type="string" column="type" length="50" nullable="false"/>
    <field name="cvLink" type="string" column="cv_link" length="100" nullable="false"/>
    <field name="imgLink" type="string" column="img_link" length="500" nullable="false"/>
  </entity>
</doctrine-mapping>
