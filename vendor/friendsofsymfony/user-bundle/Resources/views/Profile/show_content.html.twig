{% trans_default_domain 'FOSUserBundle' %}

<div class="fos_user_user_show">
    <p>{{ 'profile.show.username'|trans }}: {{ user.username }}</p>
    <p>{{ 'profile.show.email'|trans }}: {{ user.email }}</p>
    <p>{{ 'name'|trans }}: {{ user.name }}</p>
    <p>{{ 'level'|trans }}: {{ user.level }}</p>
    <p>{{ 'type'|trans }}: {{ user.type }}</p>
    <p>{{ 'Birth date'|trans }}: {{ user.birthDate | date('d/m/Y') }}</p>
    <p>{{ 'Cv link'|trans }}: {{ user.cvLink }}</p>
    <p>{{ 'Img link'|trans }}: {{ user.imgLink }}</p>
</div>
